<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <!-- Vehicle types -->
    <vType id="car" accel="2.5" decel="4.5" sigma="0.5" length="5" maxSpeed="50"/>
    <vType id="truck" accel="1.5" decel="3.5" sigma="0.3" length="12" maxSpeed="30"/>
    <vType id="bus" accel="1.8" decel="4.0" sigma="0.2" length="15" maxSpeed="25"/>
    
    <!-- Routes -->
    <route id="north_south" edges="north_in south_out"/>
    <route id="south_north" edges="south_in north_out"/>
    <route id="east_west" edges="east_in west_out"/>
    <route id="west_east" edges="west_in east_out"/>
    <route id="north_east" edges="north_in east_out"/>
    <route id="north_west" edges="north_in west_out"/>
    <route id="south_east" edges="south_in east_out"/>
    <route id="south_west" edges="south_in west_out"/>
    <route id="east_north" edges="east_in north_out"/>
    <route id="east_south" edges="east_in south_out"/>
    <route id="west_north" edges="west_in north_out"/>
    <route id="west_south" edges="west_in south_out"/>
    
    <!-- Vehicle flows -->
    <flow id="north_south_flow" type="car" route="north_south" begin="0" end="300" period="3"/>
    <flow id="south_north_flow" type="car" route="south_north" begin="5" end="300" period="4"/>
    <flow id="east_west_flow" type="car" route="east_west" begin="10" end="300" period="5"/>
    <flow id="west_east_flow" type="car" route="west_east" begin="15" end="300" period="6"/>
    <flow id="north_east_flow" type="car" route="north_east" begin="20" end="300" period="8"/>
    <flow id="south_west_flow" type="car" route="south_west" begin="25" end="300" period="7"/>
    
    <!-- Random vehicles -->
    <flow id="random_cars" type="car" route="north_south" begin="0" end="300" probability="0.3"/>
    <flow id="random_trucks" type="truck" route="east_west" begin="30" end="300" probability="0.1"/>
    <flow id="random_buses" type="bus" route="south_north" begin="60" end="300" probability="0.05"/>
</routes>