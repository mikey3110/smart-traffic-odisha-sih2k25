    <flow id="flow_west_to_south" type="passenger" route="west_to_south" begin="0" end="3600" vehsPerHour="32" color="1,1,0"/>

    <!-- Truck flows -->
    <flow id="truck_flow_north_to_south" type="truck" route="north_to_south" begin="0" end="3600" vehsPerHour="20" color="0,1,1"/>
    <flow id="truck_flow_south_to_north" type="truck" route="south_to_north" begin="0" end="3600" vehsPerHour="18" color="0,1,1"/>
    <flow id="truck_flow_east_to_west" type="truck" route="east_to_west" begin="0" end="3600" vehsPerHour="15" color="0,1,1"/>
    <flow id="truck_flow_west_to_east" type="truck" route="west_to_east" begin="0" end="3600" vehsPerHour="17" color="0,1,1"/>

    <!-- Bus flows -->
    <flow id="bus_flow_north_to_south" type="bus" route="north_to_south" begin="0" end="3600" vehsPerHour="5" color="1,0,1"/>
    <flow id="bus_flow_south_to_north" type="bus" route="south_to_north" begin="0" end="3600" vehsPerHour="5" color="1,0,1"/>
    <flow id="bus_flow_east_to_west" type="bus" route="east_to_west" begin="0" end="3600" vehsPerHour="4" color="1,0,1"/>
    <flow id="bus_flow_west_to_east" type="bus" route="west_to_east" begin="0" end="3600" vehsPerHour="4" color="1,0,1"/>

    <!-- Motorcycle flows -->
    <flow id="motorcycle_flow_north_to_south" type="motorcycle" route="north_to_south" begin="0" end="3600" vehsPerHour="10" color="0,1,0"/>
    <flow id="motorcycle_flow_south_to_north" type="motorcycle" route="south_to_north" begin="0" end="3600" vehsPerHour="8" color="0,1,0"/>
