{
  "name": "smart-traffic-dashboard",
  "version": "1.0.0",
  "description": "Smart Traffic Management Dashboard with React and SAP UI5",
  "private": true,
  "dependencies": {
    "@types/leaflet": "^1.9.20",
    "@types/lodash": "^4.14.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-virtualized": "^9.21.0",
    "@ui5/webcomponents": "^1.15.0",
    "@ui5/webcomponents-fiori": "^1.15.0",
    "@ui5/webcomponents-icons": "^1.15.0",
    "@ui5/webcomponents-react": "^1.15.0",
    "axios": "^1.3.0",
    "chart.js": "^4.2.0",
    "date-fns": "^2.29.0",
    "framer-motion": "^10.0.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.4.0",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.8.0",
    "react-virtualized": "^9.22.0",
    "typescript": "^4.9.0",
    "workbox-webpack-plugin": "^6.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@cypress/react": "^9.0.1",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-onboarding": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^30.1.2",
    "cypress": "^15.2.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.0",
    "sass": "^1.58.0",
    "storybook": "^9.1.6",
    "ts-jest": "^29.4.2",
    "vite": "^4.1.0",
    "vite-plugin-checker": "^0.10.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:all": "npm run test && npm run test:e2e && npm run test:component",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,scss}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,scss}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:build": "docker build -t smart-traffic-frontend .",
    "docker:run": "docker run -p 3000:3000 smart-traffic-frontend",
    "docker:compose": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "k8s:status": "kubectl get pods -n smart-traffic"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
