<?xml version="1.0" encoding="UTF-8"?>
<!--
Normal Traffic Routes for 4-Way Intersection
Realistic Indian urban traffic patterns with mixed vehicle types
-->
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    
    <!-- Vehicle types for Indian traffic -->
    <vType id="car" accel="2.5" decel="4.5" sigma="0.5" length="4.5" maxSpeed="50" color="1,1,0" />
    <vType id="auto_rickshaw" accel="2.0" decel="4.0" sigma="0.6" length="3.0" maxSpeed="40" color="0,1,0" />
    <vType id="motorcycle" accel="3.0" decel="5.0" sigma="0.7" length="2.0" maxSpeed="60" color="1,0,0" />
    <vType id="bus" accel="1.5" decel="3.0" sigma="0.3" length="12.0" maxSpeed="40" color="0,0,1" />
    <vType id="truck" accel="1.0" decel="2.5" sigma="0.2" length="15.0" maxSpeed="35" color="0.5,0.5,0.5" />
    <vType id="emergency" accel="3.0" decel="6.0" sigma="0.1" length="5.0" maxSpeed="80" color="1,1,1" />
    
    <!-- Route definitions -->
    <route id="north_to_south" edges="north_approach north_exit" />
    <route id="south_to_north" edges="south_approach south_exit" />
    <route id="east_to_west" edges="east_approach east_exit" />
    <route id="west_to_east" edges="west_approach west_exit" />
    
    <!-- Left turn routes -->
    <route id="north_to_west" edges="north_approach center_north north_to_west" />
    <route id="south_to_east" edges="south_approach center_south south_to_east" />
    <route id="east_to_north" edges="east_approach center_east east_to_north" />
    <route id="west_to_south" edges="west_approach center_west west_to_south" />
    
    <!-- Right turn routes -->
    <route id="north_to_east" edges="north_approach center_north_right north_to_east" />
    <route id="south_to_west" edges="south_approach center_south_right south_to_west" />
    <route id="east_to_south" edges="east_approach center_east_right east_to_south" />
    <route id="west_to_north" edges="west_approach center_west_right west_to_north" />
    
    <!-- U-turn routes -->
    <route id="north_u_turn" edges="north_approach south_exit" />
    <route id="south_u_turn" edges="south_approach north_exit" />
    <route id="east_u_turn" edges="east_approach west_exit" />
    <route id="west_u_turn" edges="west_approach east_exit" />
    
    <!-- Cross traffic routes -->
    <route id="north_to_east_cross" edges="north_approach east_exit" />
    <route id="south_to_west_cross" edges="south_approach west_exit" />
    <route id="east_to_south_cross" edges="east_approach south_exit" />
    <route id="west_to_north_cross" edges="west_approach north_exit" />
    
    <!-- Vehicle flows for normal traffic (8 AM - 10 AM) -->
    <flow id="north_to_south_cars" route="north_to_south" begin="0" end="7200" number="300" type="car" />
    <flow id="south_to_north_cars" route="south_to_north" begin="0" end="7200" number="280" type="car" />
    <flow id="east_to_west_cars" route="east_to_west" begin="0" end="7200" number="250" type="car" />
    <flow id="west_to_east_cars" route="west_to_east" begin="0" end="7200" number="270" type="car" />
    
    <!-- Auto-rickshaws (common in Indian cities) -->
    <flow id="north_to_south_autos" route="north_to_south" begin="0" end="7200" number="150" type="auto_rickshaw" />
    <flow id="south_to_north_autos" route="south_to_north" begin="0" end="7200" number="140" type="auto_rickshaw" />
    <flow id="east_to_west_autos" route="east_to_west" begin="0" end="7200" number="120" type="auto_rickshaw" />
    <flow id="west_to_east_autos" route="west_to_east" begin="0" end="7200" number="130" type="auto_rickshaw" />
    
    <!-- Motorcycles (very common in Indian traffic) -->
    <flow id="north_to_south_bikes" route="north_to_south" begin="0" end="7200" number="200" type="motorcycle" />
    <flow id="south_to_north_bikes" route="south_to_north" begin="0" end="7200" number="180" type="motorcycle" />
    <flow id="east_to_west_bikes" route="east_to_west" begin="0" end="7200" number="160" type="motorcycle" />
    <flow id="west_to_east_bikes" route="west_to_east" begin="0" end="7200" number="170" type="motorcycle" />
    
    <!-- Left turn flows -->
    <flow id="north_to_west_left" route="north_to_west" begin="0" end="7200" number="80" type="car" />
    <flow id="south_to_east_left" route="south_to_east" begin="0" end="7200" number="75" type="car" />
    <flow id="east_to_north_left" route="east_to_north" begin="0" end="7200" number="70" type="car" />
    <flow id="west_to_south_left" route="west_to_south" begin="0" end="7200" number="85" type="car" />
    
    <!-- Right turn flows -->
    <flow id="north_to_east_right" route="north_to_east" begin="0" end="7200" number="60" type="car" />
    <flow id="south_to_west_right" route="south_to_west" begin="0" end="7200" number="55" type="car" />
    <flow id="east_to_south_right" route="east_to_south" begin="0" end="7200" number="50" type="car" />
    <flow id="west_to_north_right" route="west_to_north" begin="0" end="7200" number="65" type="car" />
    
    <!-- Cross traffic flows -->
    <flow id="north_to_east_cross" route="north_to_east_cross" begin="0" end="7200" number="40" type="car" />
    <flow id="south_to_west_cross" route="south_to_west_cross" begin="0" end="7200" number="35" type="car" />
    <flow id="east_to_south_cross" route="east_to_south_cross" begin="0" end="7200" number="30" type="car" />
    <flow id="west_to_north_cross" route="west_to_north_cross" begin="0" end="7200" number="45" type="car" />
    
    <!-- Buses (public transport) -->
    <flow id="north_to_south_bus" route="north_to_south" begin="0" end="7200" number="20" type="bus" />
    <flow id="south_to_north_bus" route="south_to_north" begin="0" end="7200" number="18" type="bus" />
    <flow id="east_to_west_bus" route="east_to_west" begin="0" end="7200" number="15" type="bus" />
    <flow id="west_to_east_bus" route="west_to_east" begin="0" end="7200" number="17" type="bus" />
    
    <!-- Trucks (commercial vehicles) -->
    <flow id="north_to_south_truck" route="north_to_south" begin="0" end="7200" number="15" type="truck" />
    <flow id="south_to_north_truck" route="south_to_north" begin="0" end="7200" number="12" type="truck" />
    <flow id="east_to_west_truck" route="east_to_west" begin="0" end="7200" number="10" type="truck" />
    <flow id="west_to_east_truck" route="west_to_east" begin="0" end="7200" number="13" type="truck" />
    
    <!-- U-turn flows (common in Indian traffic) -->
    <flow id="north_u_turn" route="north_u_turn" begin="0" end="7200" number="25" type="car" />
    <flow id="south_u_turn" route="south_u_turn" begin="0" end="7200" number="20" type="car" />
    <flow id="east_u_turn" route="east_u_turn" begin="0" end="7200" number="18" type="car" />
    <flow id="west_u_turn" route="west_u_turn" begin="0" end="7200" number="22" type="car" />
    
    <!-- Pedestrian flows -->
    <flow id="ped_north_south" route="ped_cross_ns" begin="0" end="7200" number="100" type="pedestrian" />
    <flow id="ped_east_west" route="ped_cross_ew" begin="0" end="7200" number="90" type="pedestrian" />
</routes>
