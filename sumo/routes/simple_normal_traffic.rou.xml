<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <!-- Vehicle types -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="4.3" maxSpeed="13.89"/>
    <vType id="bus" accel="1.0" decel="3.0" sigma="0.3" length="12.0" maxSpeed="11.11"/>
    <vType id="motorcycle" accel="3.0" decel="6.0" sigma="0.8" length="2.0" maxSpeed="16.67"/>
    
    <!-- Routes -->
    <route id="north_south" edges="north_to_south" color="red"/>
    <route id="south_north" edges="south_to_north" color="blue"/>
    <route id="east_west" edges="east_to_west" color="green"/>
    <route id="west_east" edges="west_to_east" color="yellow"/>
    
    <!-- Vehicle flows -->
    <flow id="north_south_flow" type="car" route="north_south" begin="0" end="300" period="3"/>
    <flow id="south_north_flow" type="car" route="south_north" begin="5" end="300" period="4"/>
    <flow id="east_west_flow" type="car" route="east_west" begin="10" end="300" period="5"/>
    <flow id="west_east_flow" type="car" route="west_east" begin="15" end="300" period="6"/>
    
    <!-- Some buses -->
    <flow id="bus_flow" type="bus" route="north_south" begin="20" end="300" period="30"/>
    
    <!-- Some motorcycles -->
    <flow id="motorcycle_flow" type="motorcycle" route="east_west" begin="25" end="300" period="8"/>
</routes>